{
    "name": "ryanxedi/laravel-cookie-http",
    "description": "Stateful HTTP client for Laravel using Guzzle CookieJar",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Howard",
            "email": "ryan@xedi.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ryanxedi\\CookieHttp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryanxedi\\CookieHttp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ryanxedi\\CookieHttp\\CookieHttpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}